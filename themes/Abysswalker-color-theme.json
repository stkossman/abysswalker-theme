{
	"name": "Abysswalker",
	"type": "dark",
	"colors": {
		"focusBorder": "#1a1a1a",
		"editor.background": "#1a1a1a",
		"editor.foreground": "#a0a0a0",
		"editorLineNumber.foreground": "#333333",
		"editorLineNumber.activeForeground": "#f5f5f5",
		"editorCursor.foreground": "#f5f5f5",
		"editor.selectionBackground": "#333333",
		"list.activeSelectionForeground": "#bd7e7e",
		"list.focusOutline": "#1a1a1a",
		"list.activeSelectionBackground": "#252525",
		"list.hoverBackground": "#202020",
		"button.background": "#bd7e7e",
		"button.foreground": "#1a1a1a",
		"button.hoverBackground": "#d1c29b",
		"activityBar.background": "#1a1a1a",
		"sideBar.background": "#1a1a1a",
		"sideBar.border": "#1a1a1a",
		"titleBar.activeBackground": "#1a1a1a",
		"statusBar.background": "#1a1a1a",
		"statusBar.foreground": "#666666",
		"tab.activeBackground": "#1a1a1a",
		"tab.inactiveBackground": "#1a1a1a",
		"tab.border": "#1a1a1a",
		"tab.activeBorderTop": "#bd7e7e",
		"breadcrumb.foreground": "#444444",
		"breadcrumb.focusForeground": "#f5f5f5"
	},
	"tokenColors": [
		{
			"name": "Global Reset",
			"scope": ["source", "variable", "constant.other.object.key", "meta.object-literal.key"],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"name": "Comments - The Void",
			"scope": "comment",
			"settings": {
				"foreground": "#606060",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords - Dried Blood",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new"],
			"settings": {
				"foreground": "#bd7e7e"
			}
		},
		{
			"name": "Strings - Old Gold",
			"scope": "string",
			"settings": {
				"foreground": "#d1c29b"
			}
		},
		{
			"name": "Functions - Soul Blue",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#8fa8bd"
			}
		},
		{
			"name": "Components & Classes - Bone White (Main Focus)",
			"scope": ["support.class.component", "entity.name.type", "entity.name.class", "entity.name.tag", "jsx.tag.name"],
			"settings": {
				"foreground": "#f5f5f5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Noise Reduction (Punctuation) - Shadows",
			"scope": ["punctuation", "meta.brace", "meta.delimiter", "keyword.operator"],
			"settings": {
				"foreground": "#757575"
			}
		}
	]
}
